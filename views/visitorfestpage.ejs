<a href="/">Home</a>

<button onclick="window.location.href='/Visitorhome'">Back</button>
    </script>
<% var i%>
<% for(i=0;i<fests.competitions.length; i++){ %> 
    <div>
        Name : <%=fests.competitions[i].name%>
    </div>
    <div>
        Uploaded Image :<%=fests.competitions[i].imageUpload%>
    </div>
    <div>
        Description : <%=fests.competitions[i].descr%>
    </div>
    <div>
        Date : <%=fests.competitions[i].date%>
    </div>
    <div> 
        Start Time : <%=fests.competitions[i].starttime.getHours()%>:<%=fests.competitions[i].starttime.getMinutes()%>
        End Time : <%=fests.competitions[i].endtime.getHours()%>:<%=fests.competitions[i].endtime.getMinutes()%>
    </div>
    <div>
        Venue : <%=fests.competitions[i].venue%>
    </div>
    <form action="/Visitorhome/<%=fest%>/<%= fests.competitions[i]._id%>" method="POST">
       <%if(fests.competitions[i].type == "competitionsd" || fests.competitions[i].type == "competitionss"){%> 
             <button id="registerb" name="register" value="register">Register</button>
        <%}%>  
             <button id="scheduleb" name="schedule" value="schedule">Scheduler</button>
    </form>   
    <hr>
<% } %>


<div class="container">
    <div class="displayform">
        <%var months = [
         'Jan', 'Feb', 'Mar', 'Apr', 'May',
         'Jun', 'Jul', 'Aug', 'Sep',
         'Oct', 'Nov', 'Dec'
         ];%>
        <%var i;%>
             <table>
                 <tr>
                    <th>FEST Name</th>
                    <th>COLLEGE Name</th>
                    <th>TYPE of Fest</th>
                 </tr>
                <%for(i=0;i<fests.length;i++){ %> 
                    <tr>
                        <td>
                            <a href="/cordhome/<%= fests[i].festname %>" >
                                <span class="festdetails">
                                    <%=fests[i].festname%>
                                </span>
                               </a>
                        </td>
                          <td>
                            <span class="festdetails">
                                <%=fests[i].college%>
                            </span>
                          </td>
                           <td>
                            <span class="festdetails">
                                <%=fests[i].type%>
                            </span>
                           </td>
                            <td>
                            <span class="festdetails">
                                <%=fests[i].from.getDate() + '-' + months[fests[i].from.getMonth()] + '-' + fests[i].from.getFullYear()%>
                            </span>
                          </td>
                          <td>
                            <span class="festdetails">
                                <%=fests[i].to.getDate() + '-' + months[fests[i].to.getMonth()] + '-' + fests[i].to.getFullYear()%>
                            </span>
                          </td>
                    </tr>
                 <%}%>
             </table>
    </div>    
</div>/