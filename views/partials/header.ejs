            <%if(user == null){%>
             <li class="nav-item">
               <a class="nav-link active" href="/login" style="font-weight:400; font-size: 1.2rem;">Login</a>
             </li>  
             <li class="nav-item">
                <a class="nav-link active" href="/signup" style="font-weight:400; font-size: 1.2rem;">Sign Up</a>
             </li>   
            <%}else{%>
            <li class="nav-item">
                <form action="/logout" method="POST">
                <button id="navlogout" style="color:white; font-weight:400; font-size: 1.2rem;">Logout</button>
                </form>
